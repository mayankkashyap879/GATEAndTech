# Update the connection string to match your setup. Use sslmode=disable for local Postgres.
DATABASE_URL="postgresql://postgres:password@localhost:5433/gateandtech_dev?sslmode=disable"
SESSION_SECRET="replace-with-random-string"

# Redis (optional - graceful degradation without it)
REDIS_URL="redis://USERNAME:PASSWORD@HOST:PORT"

# Razorpay Payment Gateway
RAZORPAY_KEY_ID=""
RAZORPAY_KEY_SECRET=""
RAZORPAY_WEBHOOK_SECRET=""

# OAuth Providers
GOOGLE_CLIENT_ID=""
GOOGLE_CLIENT_SECRET=""
GITHUB_CLIENT_ID=""
GITHUB_CLIENT_SECRET=""

# Email Service (SMTP)
# If not configured, emails will be logged to console
SMTP_HOST="smtp.gmail.com"
SMTP_PORT="587"
SMTP_SECURE="false"
SMTP_USER="your-email@gmail.com"
SMTP_PASS="your-app-password"
SMTP_FROM="noreply@gateandtech.com"

# Application URL (for email links)
APP_URL="http://localhost:5000"
